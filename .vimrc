syntax enable
filetype plugin indent on
set tabstop=4
set softtabstop=4
set expandtab
set shiftwidth=4
set number
set relativenumber
set showcmd
set cursorline
filetype indent on
set wildmenu
set lazyredraw
set showmatch
set incsearch
set hlsearch

set term=screen-256color

let mapleader=";"
inoremap <leader>u <esc>
nnoremap <leader>c :nohlsearch<CR>

colorscheme moonshine_lowcontrast
